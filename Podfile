    pod 'ExpandableCell', '~> 1.2.2'
    pod 'SwiftyContacts', '~> 3.0.10'
    pod 'Firebase/Core', '~> 5.0.1'
    pod 'Firebase/Messaging', '~> 5.0.1'
    pod 'SwiftLocation', '~> 3.2.3'
end

target 'Hackfest-swvlDev' do
